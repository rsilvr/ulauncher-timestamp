{
    "required_api_version": "^2.0.0",
    "name": "Timestamp",
    "description": "Convert unix timestamp to readable date format",
    "developer_name": "Bruno Paz",
    "icon": "images/icon.png",
    "options": {
        "query_debounce": 0.1
    },
    "preferences": [
        {
            "id": "kw",
            "type": "keyword",
            "name": "Timestamp Keyword",
            "default_value": "ts"
        },
        {
            "id": "unit",
            "type": "select",
            "name": "Unix Timestamp Unit",
            "default_value": "ms",
            "options": [
                {
                    "text": "Seconds",
                    "value": "s"
                },
                {
                    "text": "Milliseconds",
                    "value": "ms"
                }
            ]
        }
    ]
}
